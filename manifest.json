{
    "manifest_version": 2,
    "name": "StackMark",
    "description": "This is our simple bookmarker Chrome Extension! WOOO!",
    "version": "0.2",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab", 
        "http://*/*", 
        "https://*/*",
        "storage"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}